<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>GEU-COVID19 Help</title>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
    <link rel="stylesheet" href="css/style.css">
</head>
<body>
    <!-- ADD YOUR GALLERY HERE -->



    <!-- ADD YOUR GALLERY ENDS -->
    
    
    <!-- FORM FOR SUBMITTING INFO STARTS-->
    <div class="container">
        <div class="row">
            <div class="col-10">
                <h2 class="display-3">List of Donors</h2>
            </div>
            <div class="col-10">
                <% for(var i=0; i < data.length; i++) { %>
                    <div class="info">
                        <div class="row">
                            <div class="col-8">
                                <span class="info-field">Donor : </span>
                                <span class="info-value"><%= data[i].name %></span>
                            </div>
                            <div class="col-8">
                                <span class="info-field">Amount : </span>
                                <span class="info-value">&#8377; <%= data[i].amount %></span> 
                            </div>
                            <div class="col-8">
                                <span class="info-field">Mode : </span>
                                <span class="info-value"><%= data[i].mode %></span>
                            </div>
                            <div class="col-8">
                                <span class="info-field">Date : </span>
                                <span class="info-value"><%= data[i].date %></span>
                            </div>
                        </div>
                    </div>
                 <% } %>
            </div>
        </div>
    </div>
    <!-- FORM FOR SUBMITTING INFO ENDS -->




</body>
